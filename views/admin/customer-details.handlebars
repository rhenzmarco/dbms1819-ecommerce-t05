  .product_row_css {
      display: -ms-flexbox; /* IE 10 */
      display: flex;
      -ms-flex-wrap: wrap; /* IE 10 */
      flex-wrap: wrap;
      padding: 0 4px;
  }

  /* Create four equal columns that sits next to each other */
  .product_column_css {
      -ms-flex: 25%; /* IE 10 */
      flex: 25%;
      padding: 0 0px;
  }

  .product_column_css img {
      margin-top: 5px;
      vertical-align: middle;
  }


  .product_container {
    position: relative;
    /*width: 275px;*/
    max-width: 275px;
    margin: 4px;
  }

  .imgPost {
    display: block;
    /*width: 100%;*/
    height: auto;
  }

  .product_overlay {
    position: absolute; 
    bottom: 0; 
    background: rgb(0, 0, 0);
    background: rgba(0, 0, 0, 0.5); /* Black see-through */
    color: #f1f1f1; 
    width: 100%;
    height: 50pt;
    transition: .5s ease;
    opacity:0;
    color: white;
    font-size: 20px;
    padding: 0px;
    text-align: center;
      cursor: pointer;
  }

  .product_container:hover .product_overlay {
    opacity: 1;
  }

  /*css for form product*/
  .form-update-button {
    -moz-box-shadow:inset 0px 1px 0px 0px #ffffff;
    -webkit-box-shadow:inset 0px 1px 0px 0px #ffffff;
    box-shadow:inset 0px 1px 0px 0px #ffffff;
    background-color:#f9f9f9;
    -moz-border-radius:6px;
    -webkit-border-radius:6px;
    border-radius:6px;
    border:1px solid #dcdcdc;
    display:inline-block;
    cursor:pointer;
    color:#666666;
    font-family:Arial;
    font-size:15px;
    font-weight:bold;
    padding:6px 24px;
    text-decoration:none;
  }
  .form-update-button:hover {
    background-color:#e9e9e9;
  }
  .form-update-button:active {
    position:relative;
    top:1px;
  }

  .form-submit-button {
    -moz-box-shadow:inset 0px -3px 7px 0px #52514e;
    -webkit-box-shadow:inset 0px -3px 7px 0px #52514e;
    box-shadow:inset 0px -3px 7px 0px #52514e;
    background-color:#52514e;
    -moz-border-radius:3px;
    -webkit-border-radius:3px;
    border-radius:3px;
    border:1px solid #52514e;
    display:inline-block;
    cursor:pointer;
    color:#ffffff;
    font-family:Arial;
    font-size:15px;
    padding:9px 23px;
    text-decoration:none;
    text-shadow:0px 1px 0px #000000;
  }
  .form-submit-button:hover {
    background-color:#52514e;
  }
  .form-submit-button:active {
    position:relative;
    top:1px;
  }



