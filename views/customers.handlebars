<style>



</style>

<div class="container-fluid">
	<div class="row">
		<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
			<div class="table-responsive">
				<table class="table table-striped table-hover table-design">
					<thead>
						<tr>
							<th>First Name</th>
							<th>Middle Name</th>
							<th>Last Name</th>
							<th>Email Address</th>
							<th>Customer Details</th>
						</tr>
					</thead>
					{{#each rows}}
					<tbody>
						<tr>
							<td>{{this.first_name}}</td>
							<td>{{this.middle_name}}</td>
							<td>{{this.last_name}}</td>
							<td>{{this.email}}</td>
							<td>
								<a href="/customers/{{this.id}}">More details&nbsp;&raquo;</a>
							</td>
						</tr>
					</tbody>
					{{/each}}
				</table>
			</div>
		</div>
	</div>
</div>